<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <ListView id="listView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <HBox id="bottomMainHBox" prefWidth="200.0">
         <children>
            <HBox id="bottomParametersHBox" minWidth="0.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Adress:">
                     <HBox.margin>
                        <Insets left="3.0" right="3.0" top="5.0" />
                     </HBox.margin>
                  </Text>
                  <TextField id="adressField" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets top="1.0" />
                     </HBox.margin>
                  </TextField>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Port:">
                     <HBox.margin>
                        <Insets left="6.0" right="3.0" top="5.0" />
                     </HBox.margin>
                  </Text>
                  <TextField id="portField" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets top="1.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <Button id="button" minWidth="-Infinity" mnemonicParsing="false" text="Connect">
               <HBox.margin>
                  <Insets bottom="1.0" left="5.0" right="1.0" top="1.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
  </children>
</VBox>
